module GooglePlaces

  class CultureItem
    attr_accessor :id,:lat,:long,:price,:rating,:name,:photo,:icon,:reference;

    def initialize(lat,long,name,rating,price,photo,icon,reference)
      @id = name+lat+long;
      @lat = lat;
      @long = long;
      @name = name;
      @rating = rating;
      @price = price;
      @photo = photo;
      @icon = icon;
      @reference = reference;
    end

    #da aggiungere la citt√† come parametro
    def get_culture_items
      #list_culture = CacheInformation.getCultureItems(city);
      list_culture_item = [];
      item1 = GooglePlaces.CultureItem.new("21","32","prova","3","3","photo","icon","a");
      item2 = GooglePlaces.CultureItem.new("21","32","prova2","3","3","photo","icon","b");

      #for item in list_culture
      # list_culture_item.append(item);
      #end
      list_culture_item.append(item1);
      list_culture_item.append(item2);
      return list_culture_item;

    end

  end

  class UtilityItem
    attr_accessor :id,:lat,:long,:price,:rating,:name,:photo,:icon,:reference;

    def initialize(lat,long,name,rating,price,photo,icon,reference)
      @id = name+lat+long;
      @lat = lat;
      @long = long;
      @name = name;
      @rating = rating;
      @price = price;
      @photo = photo;
      @icon = icon;
      @reference = reference;
    end

  end

  class EntertainmentItem
    attr_accessor :id,:lat,:long,:price,:rating,:name,:photo,:icon,:reference;

